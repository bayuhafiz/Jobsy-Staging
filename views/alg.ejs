<!DOCTYPE html>
<html>
  <head>
    <% include ./partial/head_alg %>
  </head>
  <body class="fixed-header error-page  ">
    <div class="container-xs-height full-height">
      <div class="row-xs-height">
        <div class="col-xs-height col-middle">
          <div class="header-container text-center">
          </div>
          <div class="row"><!-- 1st row -->
            <div class="col-lg-12 col-md-12 col-sm-12 btn-group btn-group-default" style="margin-top:20px;">
              <center>
              <div class="btn-group">
                <button type="button" class="btn btn-default btn-block" id="display">
                <span class="p-t-5 p-b-5">
                  <i class="fa fa-arrow-down fs-15"></i>
                </span>
                <br>
                <span class="fs-11 font-montserrat text-uppercase">Display Index</span>
                </button>
              </div>
              <div class="btn-group">
                <button type="button" class="btn btn-default btn-block" id="init">
                <span class="p-t-5 p-b-5">
                  <i class="fa fa-cloud-upload fs-15"></i>
                </span>
                <br>
                <span class="fs-11 font-montserrat text-uppercase">Initialization</span>
                </button>
              </div>
              <div class="btn-group">
                <button type="button" class="btn btn-default btn-block" id="clear">
                <span class="p-t-5 p-b-5">
                  <i class="fa fa-times fs-15"></i>
                </span>
                <br>
                <span class="fs-11 font-montserrat text-uppercase">Clear Index</span>
                </button>
              </div>
              <div class="btn-group">
                <button type="button" class="btn btn-default btn-block" id="show_logs">
                <span class="p-t-5 p-b-5">
                  <i class="pg-note fs-15"></i>
                </span>
                <br>
                <span class="fs-11 font-montserrat text-uppercase">Show Logs</span>
                </button>
              </div>
              </center>
            </div>
          </div>
        </div>
      </div>
    </div>
    <div class="pull-bottom sm-pull-bottom full-width">
      <div class="error-container">
        <div class="error-container-innner">
          <div class="text-center">
            <p class="small">Powered by</p>
            <img alt="algolia_logo" src="https://www.algolia.com/assets/algolia256x80_dark-7e8acaca6006682fe4a9015aff28a07a.png" style="max-height:60px;margin-bottom:60px;">
          </div>
        </div>
      </div>
    </div>
    <% include ./script/alg %>
  </body>
</html>
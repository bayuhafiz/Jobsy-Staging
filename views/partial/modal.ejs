    <div class="modal fade slide-right" id="EditJob" tabindex="-1" role="dialog" aria-hidden="true">
        <div class="modal-dialog modal-lg modal-dialog-userdashboard">
            <div class="modal-content-wrapper">
                <div class="modal-content outer" style="background: #FAFAFA;">
                    <div class="arrow-right"></div>
                    <button type="button" class="close" data-dismiss="modal" aria-hidden="true"><i class="pg-close fs-14"></i></button>
                    <div class="panel panel-transparent middle">
                        <div class="panel-body">
                            <div class="panel-heading" style="padding:0">
                                <div class="panel-title" style="text-transform: none;">
                                    <img src="assets/img/jobsy_logo_black.png" alt="logo" data-src="assets/img/jobsy_logo_black.png" data-src-retina="assets/img/jobsy_logo_black.png" width="100" style="margin-top:25px; margin-bottom:25px;">
                                    <div class="col-sm-12" style="padding-left: 0;">
                                        <p class="bold">EDIT JOB POSTING</p>
                                        <p>Fill the following for to create a new job Listing Be as detailer as possible - this ensures quality of your candidates</p>
                                    </div>
                                </div>
                            </div>
                            <br>
                            <form id="form-edit" role="form" autocomplete="off" method="POST" enctype="multipart/form-data">
                            <div id="editWizard">
                                <!-- Nav tabs -->
                                <ul class="nav nav-tabs nav-tabs-linetriangle nav-tabs-separator">
                                    <li class="active" id="firstTab">
                                        <a data-toggle="tab" href="#tab5"><i class="fa fa-building-o"></i> <span>Company Profile</span></a>
                                    </li>
                                    <li class="" id="secondTab">
                                        <a data-toggle="tab" href="#tab6"><i class="fa fa-info-circle"></i> <span>Job Details</span></a>
                                    </li>
                                </ul>
                                <!-- Tab panes -->
                                <br>
                                <div class="tab-content">

                                        <div class="tab-pane active slide-left" id="tab5">
                                            <div class="row">
                                                <div class="col-sm-6">
                                                    <div class="form-group form-group-default">
                                                        <label style="padding-bottom:10px;">Profile Picture</label>
                                                        <input type="file" class="form-control" id="editJobImg" name="logo">
                                                        <input type="hidden" name="oldLogo" id="oldJobImg">
                                                    </div>
                                                </div>
                                                <div class="col-sm-6">
                                                    <img id="editJobImg-preview" style="margin-bottom: 20px; max-height: 120px; max-width: 120px;">
                                                </div>
                                            </div>
                                            <div class="row">
                                                <div class="col-sm-6">
                                                    <div class="form-group form-group-default">
                                                        <label>Company Name</label>
                                                        <input type="text" name="companyName" placeholder="" class="form-control companyName" required style="  height: 39px;">
                                                    </div>
                                                </div>
                                                <div class="col-sm-6">
                                                    <div class="form-group form-group-default">
                                                        <label>Location</label>
                                                        <% include ../dropdown/location_edit %>
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="row">
                                                <div class="col-sm-12">
                                                    <div class="form-group form-group-default" style="min-height:190px;">
                                                        <label>Description</label>
                                                        <textarea id="editor1-edit" name="description"></textarea>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="tab-pane slide-left" id="tab6">
                                            <div class="row">
                                                <div class="col-sm-12">
                                                    <div class="form-group form-group-default">
                                                        <label>Job Title</label>
                                                        <input type="text" class="form-control jobTitle" name="jobTitle">
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="row">
                                                <div class="col-sm-6">
                                                    <div class="form-group form-group-default">
                                                        <label>Category</label>
                                                        <% include ../dropdown/category_edit %>
                                                    </div>
                                                </div>
                                                <div class="col-sm-6">
                                                    <div class="form-group form-group-default">
                                                        <label>Job Type</label>
                                                        <select class="full-width job-dropdown" data-init-plugin="select2" id="jobType" name="jobType" required>
                                                            <optgroup label="Position Type">
                                                                <option value="Full Time">Full Time</option>
                                                                <option value="Contract">Contract</option>
                                                                <option value="Part Time">Part Time</option>
                                                            </optgroup>
                                                        </select>
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="row">
                                                <div class="col-sm-12">
                                                    <div class="form-group form-group-default" style="min-height:190px;">
                                                    <label>Job Scope <span class="hint-text small"></label>
                                                      <textarea id="editor2-edit" name="jobScope"></textarea>
                                                    </div>
                                                </div>
                                            </div>
                                            <br />
                                            <div class="row">
                                                <div class="col-sm-12">
                                                    <div class="form-group form-group-default" style="min-height:190px;">
                                                    <label>Requirements <span class="hint-text small"></span></label>
                                                      <textarea id="editor3-edit" name="requirements"></textarea>
                                                    </div>
                                                </div>
                                            </div>
                                            <br>
                                            <p>Salary : </p>
                                            <div class="row">
                                                <div class="col-sm-7">
                                                    <div class="row">
                                                        <div class="col-sm-2 col-xs-2">
                                                          <select name="currency" class="currency" style="background: #FFF;border-color:rgba(0,0,0,0.07);height:32px;border-radius:0px;  width: 74px;">
                                                            <option value="USD">USD</option>
                                                            <option value="IDR">IDR</option>
                                                          </select>
                                                        </div>
                                                        <div class="col-sm-10 col-xs-10" style="padding-left: 0;">
                                                            <div class="input-daterange input-group">
                                                                <input name="salaryFrom" id="salary-from-edit" type="text" class="input-sm form-control salaryFrom"/>
                                                                <span class="input-group-addon">to</span>
                                                                <input name="salaryTo" id="salary-to-edit" type="text" class="input-sm form-control salaryTo" />
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="col-sm-5">
                                                    <div class="form-group salary-type">
                                                        <select name="salaryType" class="salaryType" style="background: #FFF;border-color:rgba(0,0,0,0.07);height:32px;border-radius:0px;  width: 100%;">
                                                            <option value="Monthly">Monthly</option>
                                                            <option value="Annually">Annually</option>
                                                            <option value="Daily">Daily</option>
                                                            <option value="Hourly">Hourly</option>
                                                        </select>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    <br>

                                    <ul class="pager wizard">
                                        <li class="next btn-next">
                                            <button class="btn btn-primary btn-cons pull-right" type="button">
                                                <span>Job Details</span> <i class="pg-arrow_lright_line_alt"></i>
                                            </button>
                                        </li>
                                        <li class="next finish" style="display:none;">
                                            <button class="btn btn-primary btn-cons pull-right" type="submit">
                                                <span>Save Edited</span>
                                            </button>
                                        </li>
                                        <li class="previous">
                                            <button class="btn btn-primary btn-cons pull-right" type="button">
                                                <i class="pg-arrow_left_line_alt"></i> <span>Company Profile</span>
                                            </button>
                                        </li>
                                    </ul>

                                </div>

                            </div>
                            </form>
                        </div>
                    </div>
                </div>
            </div>
        <!-- /.modal-content -->
        </div>
        <!-- /.modal-dialog -->
    </div>
<% } %>
